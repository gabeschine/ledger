namespace storage;

// An entry for the identifier of a part of a file.
table ObjectChild {
  size: ulong;
  object_id: [ubyte];
}

// The index of a chunked file.
table FileIndex {
  size: ulong;
  children: [ObjectChild];
}

root_type FileIndex;
